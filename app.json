{
  "expo": {
    "name": "AURA",
    "slug": "AURA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logotmp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logotmp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logotmp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.criscrissa.AURA"
    },
    "web": {
      "favicon": "./assets/logotmp.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-font"
    ]
  }
}
